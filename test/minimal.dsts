# A fairly minimal example of a dst assembly file
{
  bytecode [
    (load-integer 0 15)
    (load-integer 1 0)
    (load-constant 3 lookup)

    :label
    (equals 2 1 0)
    (jump-if 2 :done) 
    (add-immediate 0 0 -1)
    (get 2 3 0)
    (push 2)
    (syscall 2 0)
    (jump :label)

    :done
    (return-nil)
    
    :extra
    (push 2)
  ]
  constants [
    (def lookup "0123456789abcdef")
  ]
}
